<script>
  function hideShowFields() {
    var country = document.getElementById('country');
    if (country.value != 'US') {
      document.getElementById('state').hidden = true;
      document.getElementById('statetext').hidden = false;
      document.getElementById('statetext').value = '';
      console.log('country.value != US');
    } else {
      document.getElementById('state').hidden = false;
      document.getElementById('statetext').hidden = true;
      document.getElementById('statetext').value = 'Empty';
      console.log('country.value == US');
    }
  }
</script>

